{
    "name": "corepulse/corepulse",
    "description": "corepulse cms",
    "type": "pimcore-bundle",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "khanh",
            "email": "khanhln.c1003j@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "pimcore/pimcore": "source",
            "*": "dist"
        }
    },
    "require": {
        "pimcore/pimcore": ">=11.0",
        "starfruitjsc/validator-bundle": ">=1.0",
        "symfony/serializer-pack": ">=1.3",
        "rompetomp/inertia-bundle": ">=2.4",
        "pentatrion/vite-bundle": ">=6.3",
        "google/apiclient": "^2.15.0",
        "starfruit/builder-bundle": ">=0.7.0"
    },
    "autoload": {
        "psr-4": {
            "CorepulseBundle\\": "src/"
        }
    },
    "scripts": {
      "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "CorepulseBundle\\CorepulseBundle"
            ] 
        },
        "google/apiclient-services": [
            "SearchConsole",
            "Indexing"
        ]
    }
}